{
    "version": 2,
    "name": "DWMaker Base Printer",
    "inherits": "fdmprinter",
    "metadata": {
        "visible": false,
        "author": "dwmaker",
        "manufacturer": "DWMaker",
        "file_formats": "text/x-gcode",
        "first_start_actions": [ "MachineSettingsAction" ],

        "has_materials": true,
        "has_variants": false,
        "has_machine_quality": true,

        "preferred_quality_type": "coarse",
        "preferred_material": "generic_pla",

        "machine_extruder_trains": {
            "0": "dwmaker_base_extruder_0"
        },

        "exclude_materials": [
            "ultimaker_abs_black",
            "ultimaker_abs_blue",
            "ultimaker_abs_green",
