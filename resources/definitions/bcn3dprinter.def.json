{
  "version": 2,
  "name": "BCN3D Printer Base Description",
  "inherits": "fdmprinter",
  "metadata": {
    "visible": false,
    "author": "BCN3D Technologies",
    "manufacturer": "BCN3D Technologies",
    "preferred_material": "generic_pla",
    "preferred_variant_name": "0.4mm",
    "preferred_quality_type": "Standard",
    "variants_name": "Hotend",
    "has_machine_quality": true,
    "has_materials": true,
    "has_machine_materials": true,
    "has_variant_materials": true,
    "has_variants": true
  },
  "overrides": {
    "machine_gcode_flavor": { "default_value": "RepRap (Marlin/Sprinter)" },
    "machine_center_is_zero": { "default_value": false },
    "machine_extruder_count": { "default_value": 2 },
    "machine_nozzle_temp_enabled": { "value": true },
    "machine_heated_bed": { "default_value": true },
    "machine_head_with_fans_polygon": { "default_value": [[-22, 60], [-22, -25], [27, 60], [27, -25]]},
    "material_bed_temp_wait": { "value": true },
    "material_print_temp_wait": { "value": true },
    "material_bed_temp_prepend": { "value": true },
    "material_print_temp_prepend": { "value": true },
    "meshfix_maximum_resolution": { "value": "(speed_wall_0 + speed_wall_x) / 60" },
    "meshfix_maximum_deviation": { "value": "layer_height / 4" },
    "acceleration_enabled": { "value": true },
    "jerk_enabled": { "value": true },

    "acceleration_layer_0": { "value": "acceleration_topbottom" },
    "acceleration_support_interface": { "value": "acceleration_topbottom" },
    "adhesion_type": { "value": "'skirt'" },
    "coasting_min_volume": { "value": "coasting_volume" },
    "cool_fan_enabled": { "value": true },
    "cool_fan_full_at_height": { "value": "0 if adhesion_type == 'raft' else layer_height_0 + 4 * layer_height" },
    "gradual_support_infill_step_height": { "value": "max(5 * layer_height, 1)" },
    "infill_line_width": { "value": "machine_nozzle_size" },
    "infill_overlap": { "value": 15 },
    "infill_pattern": { "value": "'grid'" },
    "infill_wipe_dist": { "value": 0 },
    "initial_layer_line_width_factor": { "value": 120 },
    "jerk_layer_0": { "value": "jerk_topbottom" },
    "jerk_prime_tower": { "value": "jerk_wall" },
    "jerk_print": { "value": "machine_max_jerk_xy" },
    "jerk_support_interface": { "value": "jerk_topbottom" },
    "jerk_support": { "value": "jerk_wall" },
    "jerk_topbottom": { "value": "jerk_wall_x" },
    "jerk_travel": { "value": "jerk_print if magic_spiralize else machine_max_jerk_xy" },
    "jerk_wall_0": { "value": "jerk_wall * 0.5" },
    "jerk_wall": { "value": "jerk_print * 0.75" },
    "layer_height_0": { "value": "min(extruderValues('machine_nozzle_size')) / 2" },
    "layer_start_x": { "value": "machine_width/2" },
    "layer_start_y": { "value": "machine_depth" },
    "line_width": { "value": "machine_nozzle_size" },
    "material_final_print_temperature": { "value": "material_print_temperature + -2.5 if material_flow_dependent_temperature else material_print_temperature" },
    "material_flow_dependent_temperature": { "enabled": true, "value": false },
    "material_initial_print_temperature": { "value": "material_print_temperature + 0 if material_flow_dependent_temperature else material_print_temperature" },
    "multiple_mesh_overlap": { "value": "0.375 * machine_nozzle_size - xy_offset" },
    "ooze_shield_enabled": { "value": false },
    "optimize_wall_printing_order": { "value": true },
    "raft_airgap": { "value": "min(extruderValues('machine_nozzle_size')) * 0.55" },
    "raft_base_line_spacing": { "value": "extruderValue(adhesion_extruder_nr, 'machine_nozzle_size') * 7.5" },
    "raft_base_line_width": { "value": "extruderValue(adhesion_extruder_nr, 'machine_nozzle_size') * 2.5" },
    "raft_base_thickness": { "value": "extruderValue(adhesion_extruder_nr, 'machine_nozzle_size') * 0.75" },
    "raft_interface_line_width": { "value": "line_width * 1.5" },
    "raft_interface_thickness": { "value": "extruderValue(adhesion_extruder_nr, 'machine_nozzle_size') * 0.7" },
    "raft_margin": { "value": 3 },
    "retract_at_layer_change": { "value": false },
    "retraction_combing": { "value": "'all'" },
    "retraction_extrusion_window": { "value": 1 },
    "retraction_hop_after_extruder_switch_height": { "value": 5 },
    "retraction_hop_enabled": { "value": true },
    "retraction_hop_only_when_collides": { "value": true },
    "retraction_hop": { "value": "2 * layer_height" },
    "retraction_min_travel": { "value": "3.75 * machine_nozzle_size" },
    "retraction_prime_speed": { "maximum_value_warning": "machine_max_feedrate_e" },
    "retraction_retract_speed": { "value": "min(retraction_speed, machine_max_feedrate_e)", "maximum_value_warning": "machine_max_feedrate_e" },
    "skin_angles": { "value": "[45, -45]" },
    "skin_overlap": { "value": 25 },
    "skin_preshrink": { "value": "expand_skins_expand_distance" },
    "skirt_brim_minimal_length": { "value": 500 },
    "skirt_brim_speed": { "value": "speed_layer_0" },
    "skirt_line_count": { "enabled": false, "value": 1 },
    "speed_support_interface": { "value": "speed_wall" },
    "speed_topbottom": { "value": "speed_wall_x" },
    "speed_travel_layer_0": { "value": "round(speed_travel * speed_layer_0 / speed_print, 1)" },
    "speed_wall_0": { "value": "speed_wall" },
    "speed_wall_x": { "value": "round(speed_print - (speed_print - speed_wall) / 2, 1)" },
    "support_angle": { "value": 60 },
    "support_bottom_distance": { "value": "layer_height" },
    "support_bottom_height": { "value": "3 * layer_height" },
    "support_conical_min_width": { "value": 10 },
    "support_infill_rate": { "value": 15 },
    "support_interface_density": { "value": 75 },
    "support_interface_enable": { "value": false },
    "support_interface_height": { "value": "5 * layer_height" },
    "support_interface_pattern": { "value": "'lines'" },
    "support_interface_skip_height": { "value": "layer_height" },
    "support_line_width": { "value": "infill_line_width" },
    "support_offset": { "value": "machine_nozzle_size / 2" },
    "support_tower_maximum_supported_diameter": { "value": 3.0 },
    "support_xy_distance": { "value": "machine_nozzle_size * 2" },
    "support_z_distance": { "value": "max(2 * layer_height, 0.15)" },
    "top_bottom_pattern": { "value": "'zigzag'" },
    "travel_avoid_other_parts": { "value": false },
    "wall_0_inset": { "value": 0 },
    "wall_line_width_x": { "value": "line_width - 0.05" },
    "wall_line_width": { "value": "line_width" },
    "z_seam_x": { "value": "machine_width//2 if idex_print_mode == 'standard' else machine_width//4 if idex_print_mode == 'duplication' else (machine_width - 54)//4" },
    "z_seam_y": { "value": "machine_depth" }
  },
  "settings": {
    "platform_adhesion": {
      "children": {
        "purge_in_bucket_before_start": {
          "label": "Purge At Start of Print",
          "description": "Purge filamet before extruder starts printing.",
          "type": "bool",
          "default_value": true,
          "enabled": true,
          "settable_per_mesh": false,
          "settable_per_extruder": true,
          "comments": "BCN3D setting",
          "children": {
            "start_purge_distance": {
              "label": "Start Purge Distance",
              "description": "Filament distance purged before extruder starts printing.",
              "unit": "mm",
              "type": "float",
              "default_value": 10,
              "minimum_value": "0",
              "enabled": "purge_in_bucket_before_start",
              "settable_per_mesh": false,
              "settable_per_extruder": true,
              "comments": "BCN3D setting"
            },
            "start_purge_speed": {
              "label": "Start Purge Speed",
              "description": "The speed at which the filament is pushed by the extruder when purging in the purge tray.",
              "unit": "mm/s",
              "type": "float",
              "default_value": 30,
              "value": "round(max(40 * (machine_nozzle_size / material_diameter) ** 2, machine_nozzle_size * layer_height * speed_infill / (math.pi * ((material_diameter / 2) ** 2))), 2)",
              "minimum_value": "0.0001",
              "maximum_value_warning": "45",
              "maximum_value": "machine_max_feedrate_e",
              "enabled": "purge_in_bucket_before_start",
              "settable_per_mesh": false,
              "settable_per_extruder": true,
              "comments": "BCN3D setting"
            },
            "purge_in_bucket_before_start_gcode": {
              "label": "Purge in bucket before start GCode",
              "description": "GCode to purge in bucket each extruder.",
              "type": "str",
              "default_value": "",
              "value": "'\\nG92 E0 \\t\\t;zero the extruded length\\nG1 E{start_purge_distance} F{start_purge_speed}\\nG92 E0\\nG4 S2 \\t\\t;stabilize hotend\\nG1 E-0.5 F1000\\nG92 E0\\n'",
              "resolve": "None if purge_in_bucket_before_start else ';'",
              "enabled": false,
              "settable_per_mesh": false,
              "settable_per_extruder": true,
              "comments": "BCN3D setting"
            }
          }
        }
      }
    },
    "dual": {
      "children": {
        "idex_print_mode": {
          "label": "IDEX Print Mode",
          "description": "The IDEX print mode. You can only set this if just the left nozzle is enabled.",
          "type": "enum",
          "options": {
            "standard": "Standard",
            "duplication": "Duplication",
            "mirror": "Mirror"
          },
          "default_value": "standard",
          "enabled": true,
          "resolve": "'standard' if [1] in extruderValues('extruder_nr') else None",
          "settable_per_extruder": false,
          "comments": "BCN3D setting"
        },
        "purge_in_bucket": {
          "label": "Purge At Tool Change",
          "description": "Add an extra prime amount in the bucket to compensate oozed material while the Extruder was idle. Disable Prime tower to save time and filament.",
          "type": "bool",
          "default_value": true,
          "enabled": "idex_print_mode == 'standard'",
          "settable_per_extruder": true,
          "comments": "BCN3D setting",
          "children": {
            "purge_distance": {
              "label": "Purge Distance",
              "description": "Filament distance purged at extruder change.",
              "unit": "mm",
              "type": "float",
              "default_value": 2.5,
              "minimum_value": "0",
              "maximum_value_warning": "50",
              "enabled": "purge_in_bucket and idex_print_mode == 'standard'",
              "settable_per_mesh": false,
              "settable_per_extruder": true,
              "comments": "BCN3D setting"
            },
            "purge_speed": {
              "label": "Purge Speed",
              "description": "The speed at which the filament is pushed by the extruder when purging in the purge tray.",
              "unit": "mm/s",
              "type": "float",
              "default_value": 100,
              "minimum_value": "0.0001",
              "maximum_value": "machine_max_feedrate_e",
              "enabled": "purge_in_bucket and idex_print_mode == 'standard'",
              "settable_per_mesh": false,
              "settable_per_extruder": true,
              "comments": "BCN3D setting"
            },
            "purge_in_bucket_at_switch_gcode": {
              "label": "Purge in bucket before start GCode",
              "description": "GCode to purge in bucket each extruder.",
              "type": "str",
              "default_value": "",
              "value": "'G92 E-{switch_extruder_retraction_amount}\\nG1 E0 F{retraction_speed} ;Restore E position\\nG1 E{purge_distance} F{purge_speed} ;default purge\\nG92 E0\\nG4 S2\\nG1 E-{switch_extruder_retraction_amount} F{retraction_speed}\\nG92 E0\\n'",
              "resolve": "None if purge_in_bucket and idex_print_mode == 'standard' else ';'",
              "enabled": false,
              "settable_per_mesh": false,
              "settable_per_extruder": true,
              "comments": "BCN3D setting"
            }
          }
        }
      }
    }
  }
}
