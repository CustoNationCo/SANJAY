    "overrides": {
        "machine_gcode_flavor": {
            "default_value": "RepRap (RepRap)"
        },

        "relative_extrusion": {
            "value": true
        },

        "machine_start_gcode": {
            "default_value": ";---start-gcode---\nG28 ; home head\nM98 P\"0:/macros/Probe/Bltouch Start Script\" ; probe bed\nM220 S100 ;reset feedrate factor\nM221 S100 ;reset extruder factor\nM290 R0 S0 ;rest babystepping\nG21 ;metric values\nG90 ;absolute positioning\nM83 ;relative extrusion\nM107 ;start with the fan off\nM190 S{material_bed_temperature_layer_0} ;set bed temperature and wait\nM109 S{material_print_temperature_layer_0} ;set nozzle temperature and wait\nG1 X15.0 Y2.0 F4000 ;move X/Y to front of printer\nG0 Z30.0 F9000 ;move the platform to 30mm\nG1 F180 E+25.0 ;extrude 25 mm of feed stock\nG1 F90 E+15.0 ;extrude 15 mm of feed stock\nG1 F1800 E-10.0 ;retract filament a bit\nG1 F1800 E+11.0 ;restore filament\nG1 X90 Z0.2.5 F9000 ;lower head to plate\nG91 ;temporary set to relative mode\nG90 ;set back to absolute mode\nG1 X110.0 E4.0 F240 ;wipe head\nG1 X115.0 F4200 ;wipe faster\nG1 Z3.0 F9000 ;raise head\n;---start-gcode---\n"
        },
        "machine_end_gcode": {
            "default_value": "; --- stop gcode ---\nM104 S0 ;extruder heater off\nM140 S0 ;heated bed heater off (if you have it)\nG91 ;relative positioning\nM83 ;relative extrusion\nG1 E-20 F1800  ;retract the filament a bit before lifting the nozzle, to release some of the pressure\nG1 Z+1 X-20 Y-20 F9000 ;move Z up a bit\nG90 ;absolute positioning\nG28 Z ; home Z\nG28 X Y ;move X/Y to min endstops, so the head is out of the way\nM84 ;stop holding\n; --- stop gcode ---\n"
        },

        "machine_max_feedrate_x": {
            "value": 300
        },
        "machine_max_feedrate_y": {
            "value": 300
        },
        "machine_max_feedrate_z": {
            "value": 40
        },
        "machine_max_feedrate_e": {
            "value": 50
        },

        "machine_max_acceleration_x": {
            "value": 3000
        },
        "machine_max_acceleration_y": {
            "value": 3000
        },
        "machine_max_acceleration_z": {
            "value": 100
        },
        "machine_max_acceleration_e": {
            "value": 10000
        },
        "machine_acceleration": {
            "value": 3000
        },
        "acceleration_print": {
            "value": 3000
        },
        "acceleration_travel": {
            "value": 3000
        },

        "machine_max_jerk_xy": {
            "value": 20
        },
        "machine_max_jerk_z": {
            "value": 0.4
        },
        "machine_max_jerk_e": {
            "value": 5
        },

        "machine_heated_bed": {
            "default_value": true
        },

        "material_diameter": {
            "default_value": 1.75
        },

        "optimize_wall_printing_order": {
            "value": "True"
        },

        "retract_at_layer_change": {
            "value": "True"
        },
        "travel_avoid_other_parts": {
            "value": false
        },
        "travel_avoid_supports": {
            "value": true
        },
        "travel_retract_before_outer_wall": {
            "value": true
        },

        "meshfix_maximum_resolution": {
            "value": "(speed_wall_0 + speed_wall_x) / 60"
        },
        "meshfix_maximum_travel_resolution": {
            "value": 0.5
        },
        "meshfix_maximum_deviation": {
            "value": "layer_height / 4"
        }
    }
}
